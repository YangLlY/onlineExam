<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.edu.zjnu.OnlineExam.Beans.AdminUser" table="adminuser">
		<id name="id" type="java.lang.Integer">
			<column name="AU_ID" />
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="AU_NAME" />
		</property>
		<property name="phone" type="java.lang.String">
			<column name="AU_PHONE" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="AU_EMAIL" />
		</property>
		<property name="au_Desc" type="java.lang.String">
			<column name="AU_DESC" />
		</property>
		<property name="username" type="java.lang.String">
			<column name="AU_USERNAME" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="AU_PASSWORD" />
		</property>
		<property name="createTime" type="java.util.Date">
			<column name="AU_CREATETIME" />
		</property>
		<property name="state" type="java.lang.String">
			<column name="AU_STATE" />
		</property>
		<set name="eps" table="exampage" inverse="true" lazy="true" cascade="save-update">
			<key>
				<column name="AU_ID" />
			</key> 
			<one-to-many class="cn.edu.zjnu.OnlineExam.Beans.ExamPage" />
		</set>
		
		<set name="scs" table="stuclass"  inverse="true" lazy="true" cascade="save-update">
			<key column="AU_ID"></key>
			<one-to-many class="cn.edu.zjnu.OnlineExam.Beans.StuClass"/>
		</set>
	</class>
</hibernate-mapping>
